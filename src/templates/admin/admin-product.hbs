{{set root=".."}}
{{>admin-header title="판매 관리" cssName="admin-product"}}
<section class="admin title my-writing-title">
	<span>판매 관리</span>
</section>
<section class="menu-category">
	<ul>
		<li menu-category-detail="complete" class="active">
			판매 완료
		</li>
		<li menu-category-detail="processing">
			거래 중
		</li>
		<li menu-category-detail="complain">
			신고 리스트
		</li>
	</ul>
</section>
<section class="contents-wrapper admin-product">
	<div class="list-search-box">
		<input type="text" class="form-control" placeholder="Search">
		<i class="fa fa-search"></i>
	</div>
	<div class="admin list-header">
		<div class="product-count">
			총 <span></span>건
		</div>
		<div class="all-selected">
			<span>전체 선택</span>
			<div check-box="" id="all-selected" get-checked="false"></div>
		</div>
	</div>
	<section class="complete">
		<ul class="list product-list">
		</ul>
		<section class="more-list">
			More
			<span class="fa fa-angle-down"></span>
		</section>
	</section>
	<section class="processing">
		<ul class="list product-list">
		</ul>
		<section class="more-list">
			More
			<span class="fa fa-angle-down"></span>
		</section>
		<section class="resell-btn sell-complete">
			판매 완료
		</section>
	</section>
	<section class="complain">
		<ul class="list product-list">
		</ul>
		<section class="more-list">
			More
			<span class="fa fa-angle-down"></span>
		</section>
		<section class="resell-btn list-blind">
			Blind <i class="fa fa-eye-slash"></i>
		</section>
	</section>
</section>
<div class="popup-layer selling-complete">
	<div class="popup-text-area">
		<div class="popup-text-body">
			<div class="text1">
				판매를 완료하시겠습니까?
			</div>
		</div>
	</div>
	<div class="popup-btn-area">
		<div class="popup-btn btn-cancel">취소</div>
		<div class="popup-btn btn-ok">확인</div>
	</div>
</div>
<div class="popup-layer list-blind">
	<div class="popup-text-area">
		<div class="popup-text-body">
			<div class="text1">
				해당 글을 블라인드 처리하시겠습니까?
			</div>
		</div>
	</div>
	<div class="popup-btn-area">
		<div class="popup-btn btn-cancel">취소</div>
		<div class="popup-btn btn-ok">확인</div>
	</div>
</div>
{{>footer jsName="admin-product"}}
