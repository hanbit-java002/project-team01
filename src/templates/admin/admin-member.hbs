{{set root=".."}}
{{>admin-header title="회원 관리" cssName="admin-member"}}

<section class="admin title member-title">
	<span>회원 관리</span>
</section>

<section class="contents-wrapper admin-member">
	<div class="list-search-box">
		<input type="text" class="form-control" placeholder="Search">
		<i class="fa fa-search"></i>
	</div>
	<div class="input-style">
		<div class="dropdown">
			<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
			Rank
			<span class="caret"></span>
			</button>
			<ul class="dropdown-menu">
				<li><a href="#">일반</a></li>
				<li><a href="#">VIP</a></li>
				<li><a href="#">관리자</a></li>
			</ul>
		</div>
	</div>
	<div class="admin list-header">
		<div class="product-count">
			총 <span>15</span>건
		</div>
		<div class="array-area">
			<ul class="descending-order">
				<li class="price-low active">
					Sell
					<i class="fa fa-angle-up" aria-hidden="true"></i>
				</li>
				<li class="price-high">
					Buy
					<i class="fa fa-angle-up" aria-hidden="true"></i>
				</li>
			</ul>
		</div>
	</div>
	<section class="admin-member-list">
		<table class="table table-striped table-hover table-condensed">
			<thead>
			<tr>
				<th>No</th>
				<th>Name</th>
				<th>E-Mail</th>
				<th>Rank</th>
				<th>Sell</th>
				<th>Buy</th>
			</tr>
			</thead>
			<tbody>
			<tr>
				<td>No</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Rank</td>
				<td>Sell</td>
				<td>Buy</td>
			</tr>
			<tr>
				<td>No</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Rank</td>
				<td>Sell</td>
				<td>Buy</td>
			</tr>
			<tr>
				<td>No</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Rank</td>
				<td>Sell</td>
				<td>Buy</td>
			</tr>
			<tr>
				<td>No</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Rank</td>
				<td>Sell</td>
				<td>Buy</td>
			</tr>
			<tr>
				<td>No</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Rank</td>
				<td>Sell</td>
				<td>Buy</td>
			</tr>
			<tr>
				<td>No</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Rank</td>
				<td>Sell</td>
				<td>Buy</td>
			</tr>
			</tbody>
		</table>
	</section>
	<ul class="paging">
		<li page="firstPage">
			<a href='#'>
				<i class="fa fa-angle-double-left"></i>
			</a>
		<li page="(startPage - 1)">
			<a href="#">
				<i class="fa fa-angle-left"></i>
			</a>
		</li>
		<li page="i" class="active">
			<a href="#">
				1
			</a>
		</li>
		<li page="i">
			<a href="#">
				2
			</a>
		</li>
		<li page="i">
			<a href="#">
				3
			</a>
		</li>
		<li page="i">
			<a href="#">
				4
			</a>
		</li>
		<li page="(endPage + 1)">
			<a href="#">
				<i class="fa fa-angle-right"></i>
			</a>
		</li>
		<li page="lastPage">
			<a href="#">
				<i class="fa fa-angle-double-right"></i>
			</a>
		</li>
	</ul>
	<section class="resell-btn add-member">
		Add
	</section>
</section>

