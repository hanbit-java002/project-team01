{{set root=".."}}
{{>header title="마켓" cssName="market"}}
<section class="title market-title">
	<span>Market</span>
</section>
<section class="menu-category">
	<ul>

	</ul>
</section>
<section class="contents-wrapper">
	<section class="filter">
		<ul>
			<ul>
				<li class="filter-search active">
					<input type="text" class="form-control filter-search-box" placeholder="검색">
					<i class="fa fa-search"></i>
				</li>
				<li s-value="series-all" class="filter-series active">
					<span>Series</span>
					<i class="glyphicon glyphicon-th-large"></i>
				</li>
			</ul>
			<ul class="filter-dropdown">
				<li>
					<div class="input-style dropdown-category">
						<div class="dropdown">
							<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								<div s-value="category-all" class="dropdown-selected">Category</div> <span class="caret"></span>
							</button>
							<ul class="dropdown-menu">

							</ul>
						</div>

					</div>
				</li>
				<li>
					<div class="input-style dropdown-size">
						<div class="dropdown">
							<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								<div s-value="size-all" class="dropdown-selected">Size</div> <span class="caret"></span>
							</button>
							<ul class="dropdown-menu">

							</ul>
						</div>
					</div>
				</li>
				<li>
					<div class="input-style dropdown-quality">
						<div class="dropdown">
							<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								<div s-value="quality-all" class="dropdown-selected">Quality</div> <span class="caret"></span>
							</button>
							<ul class="dropdown-menu">
								<li value="quality-all"><a>ALL</a></li>
								<li value="new"><a>새상품</a></li>
								<li value="used"><a>중고</a></li>
							</ul>
						</div>
					</div>
				</li>
			</ul>
		</ul>
	</section>
	<div class="market-contents list-header">
		<div class="main-product-name active">
			Products
		</div>
		<ul class="product-price-order">
			<li class="price-low">
				Low Price
			</li>
			<li class="price-high">
				High Price
			</li>
		</ul>
	</div>
	<ul class="list">
		<li product-id="#">
			<div class="product-info">
				<img class="product-img" src="{{root}}/img/shoes.jpg">
				<div class="detail-info">
					<div class="product name">
						에어 조던 14 Black Pink New
					</div>
					<div class="product price">
						<i class="fa fa-won"></i>
						200,000
					</div>
					<div class="product size">
						235mm
					</div>
					<div class="product quality">
						새제품
					</div>
					<ul class="product dealing-mode">
						<li class="dealing-mode directly">
							직접거래
						</li>
						<li class="dealing-mode delivery">
							택배거래
						</li>
						<li class="dealing-mode safety">
							안심결제
						</li>
					</ul>
					<div class="seller-info">
						<div class="info-label">판매자</div>
						<div class="seller-rank fa fa-star"></div>
						<div class="seller-name">
							수현이당당다랑
						</div>
					</div>
				</div>
			</div>
			<div class="board-info">
				<div class="like">
					<span class="fa fa-heart-o"></span>
					10
				</div>
				<div class="comment">
					<span class="fa fa-commenting-o"></span>
					10
				</div>
				<div class="complain">
					<span class="fa fa-thumbs-o-down"></span>
					10
				</div>
				<div class="hits">
					<span class="fa fa-eye"></span>
					1,000
				</div>
				<div class="reporting-date">
					<span class="fa fa-clock-o"></span>
					2017/03/31
				</div>
			</div>

			<div class="list-selector like fa fa-heart" valid="true"></div>
