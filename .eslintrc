{
    "extends": "google",

	"rules": {
		"quotes": [
			"error",
			"double"
		],
		"require-jsdoc": ["error", {
			"require": {
				"FunctionDeclaration": false,
				"MethodDefinition": false,
				"ClassDeclaration": false,
				"ArrowFunctionExpression": false
			}
		}],
		"no-var": "off",
		"brace-style" : [
			"error",
			"stroustrup"
		],
		"max-len": ["error", {
			"code": 200,
			"tabWidth": 4,
			"ignoreUrls": true,
			"ignorePattern": /^goog\.(module|require)/
		}],
		"linebreak-style": "off",
		"space-before-function-paren": "off",
		"spaced-comment": "off",
		"comma-dangle": "off",
		"padded-blocks": "off",
		"no-multiple-empty-lines": "off",
		"valid-jsdoc": "off"
	}
}
